{
	"include": ["civetdist/*"],
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"resolveJsonModule": true,
		"outDir": "dist",
		"declaration": true
	}
}
